{
  "name": "simplestore",
  "version": "1.0.0",
  "description": "A simple store especially made for react",
  "main": "index.js",
  "author": "Saugat Pachhai",
  "license": "MIT",
  "scripts": {
    "init": "mkdir dist",
    "clean": "rm -rf dist",
    "build": "babel ./src -d ./dist"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-preset-env": "1.7.0"
  },
  "files": [
    "dist"
  ],
  "babel": {
    "presets": [
      ["env", {
        "targets": {
          "node": 8,
          "ie": 8
        }
      }]
    ]
  }
}
